{
  "description": "Pipeline for parsing messages",
  "processors": [
    {
      "grok": {
        "field": "message",
        "patterns": [
          "\\A%{TIMESTAMP_ISO8601:timestamp}%{SPACE}%{LOGLEVEL:level}%{SPACE}%{NOTSPACE:thread}%{SPACE}%{NOTSPACE:class}%{SPACE}%{NOTSPACE}%{SPACE}%{JAVALOGMESSAGE:message}"
        ],
        "ignore_missing": true
      }
    },
    {
      "remove": {
        "field": "message"
      }
    },
    {
      "rename": {
        "field": "system.syslog.message",
        "target_field": "message",
        "ignore_missing": true
      }
    },
    {
      "date": {
        "field": "system.syslog.timestamp",
        "target_field": "@timestamp",
        "formats": [
          "MMM  d HH:mm:ss",
          "MMM dd HH:mm:ss",
          "yyyy-MM-dd'T'HH:mm:ss.SSSSSSZZ"
        ],
        "ignore_failure": true
      }
    },
    {
      "remove": {
        "field": "system.syslog.timestamp"
      }
    }
  ]
}